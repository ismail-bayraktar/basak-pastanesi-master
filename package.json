{
  "name": "tulumbak-monorepo",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "description": "TulumBak E-commerce Turborepo Monorepo",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "test": "node scripts/test-local.js",
    "test:build": "pnpm build && echo 'Build test passed!'"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "latest",
    "turbo-ignore": "^2.6.1",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@8.15.8",
  "dependencies": {
    "mongoose": "^8.9.2"
  }
}