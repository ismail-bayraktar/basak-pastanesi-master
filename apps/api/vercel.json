{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "basakpastanesi-api",
  "version": 2,
  "buildCommand": "echo 'No build step required'",
  "ignoreCommand": "npx turbo-ignore",
  "outputDirectory": ".",
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}